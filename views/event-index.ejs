<%- include("partials/header.ejs") %>
<h1>Index Page!!</h1>

<% events.forEach(function(event){ %>
	<div>
		<h2> <%= event.name %></h2>
		<span> <%= event.host %></span>
		<span> <%= event.prize %> </span>
		<a href="/events/<%= event._id %>">Read more...</a>
	</div>
<% }) %>
